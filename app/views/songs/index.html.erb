<h1>Songs</h1>
<table>
  <tr>
    <thead>   Title   </thead>
    <thead>   Artist  </thead>
    <thead>   Year Released   </thead>
    <thead>   Genre           </thead>
  </tr>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td><%= song.artist_name%></td>
        <td><%= song.release_year if song.released? %></td>
        <td><%= song.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>
